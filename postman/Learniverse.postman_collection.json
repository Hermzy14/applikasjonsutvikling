		{
			"name": "Order a course",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201 or 200\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([201, 200]);",
							"});",
							"",
							"pm.test(\"Order ID was received and saved from text response\", function () {",
							"    const responseText = pm.response.text(); // Get the response as plain text",
							"    pm.expect(responseText).to.include(\"Order created successfully, ID:\");",
							"",
							"    // Use a regular expression to find the ID",
							"    const match = responseText.match(/ID: (\\d+)/);",
							"",
							"    if (match && match[1]) {",
							"        const orderId = match[1];",
							"        pm.collectionVariables.set(\"createdOrderId\", orderId);",
							"        console.log(\"Saved Order ID: \" + pm.collectionVariables.get(\"createdOrderId\"));",
							"        pm.expect(orderId).to.not.be.empty; // Ensure an ID was actually captured",
							"    } else {",
							"        // Fail the test if the ID pattern isn't found",
							"        pm.expect.fail(\"Could not parse Order ID from response: \" + responseText);",
							"    }",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
